      </itext>
      <instance>
        <ews_daily_register id="ews_daily_register" version="1">
          <start/>
          <end/>
          <today/>
          <deviceid/>
          <subscriberid/>
          <simid/>
          <phonenumber/>
          <intro.>
            <report_date/>
            <open/>
            <closed_reason/>
          </intro.>
          <staff.>
            <nurses/>
            <doctors/>
          </staff.>
          <caseload.>
            <new_admissions/>
            <lt_5_yo/>
            <lt_10_yo/>
            <women/>
            <out_of_region/>
            <inpatients/>
            <bed_availability/>
            <deaths/>
          </caseload.>
          <meta>
            <instanceID/>
          </meta>
        </ews_daily_register>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/ews_daily_register/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/ews_daily_register/end" type="dateTime"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/ews_daily_register/today" type="date"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/ews_daily_register/deviceid" type="string"/>
      <bind jr:preload="property" jr:preloadParams="subscriberid" nodeset="/ews_daily_register/subscriberid" type="string"/>
      <bind jr:preload="property" jr:preloadParams="simserial" nodeset="/ews_daily_register/simid" type="string"/>
      <bind jr:preload="property" jr:preloadParams="phonenumber" nodeset="/ews_daily_register/phonenumber" type="string"/>
      <bind constraint=". &lt;= today()" jr:constraintMsg="jr:itext('/ews_daily_register/intro./report_date:jr:constraintMsg')" nodeset="/ews_daily_register/intro./report_date" type="date"/>
      <bind nodeset="/ews_daily_register/intro./open" required="true()" type="select1"/>
      <bind nodeset="/ews_daily_register/intro./closed_reason" relevant=" /ews_daily_register/intro./open =&quot;no&quot;" required="true()" type="select1"/>
      <bind nodeset="/ews_daily_register/staff./nurses" relevant=" /ews_daily_register/intro./open =&quot;yes&quot;" required="true()" type="int"/>
      <bind nodeset="/ews_daily_register/staff./doctors" relevant=" /ews_daily_register/intro./open =&quot;yes&quot;" required="true()" type="int"/>
      <bind nodeset="/ews_daily_register/caseload." relevant=" /ews_daily_register/intro./open =&quot;yes&quot;"/>
      <bind nodeset="/ews_daily_register/caseload./new_admissions" required="true()" type="int"/>
      <bind constraint=". &lt;=../new_admissions" jr:constraintMsg="jr:itext('/ews_daily_register/caseload./lt_5_yo:jr:constraintMsg')" nodeset="/ews_daily_register/caseload./lt_5_yo" required="true()" type="int"/>
      <bind constraint=". &lt;=../new_admissions" jr:constraintMsg="jr:itext('/ews_daily_register/caseload./lt_10_yo:jr:constraintMsg')" nodeset="/ews_daily_register/caseload./lt_10_yo" required="true()" type="int"/>
      <bind constraint=". &lt;=../new_admissions" jr:constraintMsg="jr:itext('/ews_daily_register/caseload./women:jr:constraintMsg')" nodeset="/ews_daily_register/caseload./women" required="true()" type="int"/>
      <bind constraint=". &lt;=../new_admissions" jr:constraintMsg="jr:itext('/ews_daily_register/caseload./out_of_region:jr:constraintMsg')" nodeset="/ews_daily_register/caseload./out_of_region" required="true()" type="int"/>
      <bind nodeset="/ews_daily_register/caseload./inpatients" required="true()" type="int"/>
      <bind nodeset="/ews_daily_register/caseload./bed_availability" required="true()" type="int"/>
      <bind constraint=". &lt;=../new_admissions" jr:constraintMsg="jr:itext('/ews_daily_register/caseload./deaths:jr:constraintMsg')" nodeset="/ews_daily_register/caseload./deaths" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/ews_daily_register/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="theme-grid">
    <group ref="/ews_daily_register/intro.">
      <input appearance="no-calendar" ref="/ews_daily_register/intro./report_date">
        <label ref="jr:itext('/ews_daily_register/intro./report_date:label')"/>
      </input>
      <select1 appearance="w2" ref="/ews_daily_register/intro./open">
        <label ref="jr:itext('/ews_daily_register/intro./open:label')"/>
        <item>
          <label ref="jr:itext('/ews_daily_register/intro./open/no:label')"/>
          <value>no</value>
        </item>
        <item>
          <label ref="jr:itext('/ews_daily_register/intro./open/yes:label')"/>
          <value>yes</value>
        </item>
      </select1>
      <select1 appearance="w2" ref="/ews_daily_register/intro./closed_reason">
        <label ref="jr:itext('/ews_daily_register/intro./closed_reason:label')"/>
        <item>
          <label ref="jr:itext('/ews_daily_register/intro./closed_reason/training:label')"/>
          <value>training</value>
        </item>
        <item>
          <label ref="jr:itext('/ews_daily_register/intro./closed_reason/patients:label')"/>
          <value>patients</value>
        </item>
        <item>
          <label ref="jr:itext('/ews_daily_register/intro./closed_reason/staff:label')"/>
          <value>staff</value>
        </item>
        <item>
          <label ref="jr:itext('/ews_daily_register/intro./closed_reason/other:label')"/>
          <value>other</value>
        </item>
      </select1>
    </group>
    <group ref="/ews_daily_register/staff.">
      <input ref="/ews_daily_register/staff./nurses">
        <label ref="jr:itext('/ews_daily_register/staff./nurses:label')"/>
      </input>
      <input ref="/ews_daily_register/staff./doctors">
        <label ref="jr:itext('/ews_daily_register/staff./doctors:label')"/>
      </input>
    </group>
    <group appearance="field-list" ref="/ews_daily_register/caseload.">
      <input appearance="w2" ref="/ews_daily_register/caseload./new_admissions">
        <label ref="jr:itext('/ews_daily_register/caseload./new_admissions:label')"/>
      </input>
      <input appearance="w2" ref="/ews_daily_register/caseload./lt_5_yo">
        <label ref="jr:itext('/ews_daily_register/caseload./lt_5_yo:label')"/>
      </input>
      <input appearance="w2" ref="/ews_daily_register/caseload./lt_10_yo">
        <label ref="jr:itext('/ews_daily_register/caseload./lt_10_yo:label')"/>
      </input>
      <input appearance="w2" ref="/ews_daily_register/caseload./women">
        <label ref="jr:itext('/ews_daily_register/caseload./women:label')"/>
      </input>
      <input appearance="w2" ref="/ews_daily_register/caseload./out_of_region">
        <label ref="jr:itext('/ews_daily_register/caseload./out_of_region:label')"/>
      </input>
      <input appearance="w2" ref="/ews_daily_register/caseload./inpatients">
        <label ref="jr:itext('/ews_daily_register/caseload./inpatients:label')"/>
      </input>
      <input appearance="w2" ref="/ews_daily_register/caseload./bed_availability">
        <label ref="jr:itext('/ews_daily_register/caseload./bed_availability:label')"/>
      </input>
      <input appearance="w2" ref="/ews_daily_register/caseload./deaths">
        <label ref="jr:itext('/ews_daily_register/caseload./deaths:label')"/>
      </input>
    </group>
  </h:body>
</h:html>
